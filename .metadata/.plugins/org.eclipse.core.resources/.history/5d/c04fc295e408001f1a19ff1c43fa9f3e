package homwork04.basic.person;
// 2.
public class Parent {

	// 성씨
	String firstName;
	// 혈액형
	String bloodType;
	// 피부색
	String skinColor;
	
	public Parent() {
		
	}

	public Parent(String firstName, String bloodType, String skinColor) {
		this.firstName = firstName;
		this.bloodType = bloodType;
		this.skinColor = skinColor;
	}
	
	
}
