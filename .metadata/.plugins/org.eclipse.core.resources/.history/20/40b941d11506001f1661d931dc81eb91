package homework.middle;

public class middleTest05 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 6-1
		for(int i = 0; i < 5; i++) {
			for(int j = 0; j < i + 1; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		// 6-2
		for(int i = 0; i < 5; i++) {
			for(int j = 5; j < i; j--) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
	
}
